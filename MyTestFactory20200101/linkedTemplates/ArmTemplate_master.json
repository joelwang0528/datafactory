{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MyTestFactory20200101"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"blob storage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob storage'"
		},
		"datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake'"
		},
		"adf_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adfkv20200701.vault.azure.net/"
		},
		"datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nswtransportdata.dfs.core.windows.net"
		},
		"trigger_every_30_sec_properties_pipeline_parameters_FolderPath_Source": {
			"type": "string",
			"defaultValue": "siri"
		},
		"trigger_every_30_sec_properties_pipeline_parameters_Directory_Source": {
			"type": "string",
			"defaultValue": "consumer"
		},
		"trigger_every_30_sec_properties_pipeline_parameters_FolderPath_Destination": {
			"type": "string",
			"defaultValue": "nswtransportdata"
		},
		"trigger_every_30_sec_properties_pipeline_parameters_Directory_Destination": {
			"type": "string",
			"defaultValue": "raw/PTMS"
		},
		"trigger_every_30_sec_properties_pipeline_parameters_LastModified_From": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"trigger_every_30_sec_properties_pipeline_parameters_LastModified_To": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"blob storage_connectionString": {
						"value": "[parameters('blob storage_connectionString')]"
					},
					"datalake_accountKey": {
						"value": "[parameters('datalake_accountKey')]"
					},
					"adf_properties_typeProperties_baseUrl": {
						"value": "[parameters('adf_properties_typeProperties_baseUrl')]"
					},
					"datalake_properties_typeProperties_url": {
						"value": "[parameters('datalake_properties_typeProperties_url')]"
					},
					"trigger_every_30_sec_properties_pipeline_parameters_FolderPath_Source": {
						"value": "[parameters('trigger_every_30_sec_properties_pipeline_parameters_FolderPath_Source')]"
					},
					"trigger_every_30_sec_properties_pipeline_parameters_Directory_Source": {
						"value": "[parameters('trigger_every_30_sec_properties_pipeline_parameters_Directory_Source')]"
					},
					"trigger_every_30_sec_properties_pipeline_parameters_FolderPath_Destination": {
						"value": "[parameters('trigger_every_30_sec_properties_pipeline_parameters_FolderPath_Destination')]"
					},
					"trigger_every_30_sec_properties_pipeline_parameters_Directory_Destination": {
						"value": "[parameters('trigger_every_30_sec_properties_pipeline_parameters_Directory_Destination')]"
					},
					"trigger_every_30_sec_properties_pipeline_parameters_LastModified_From": {
						"value": "[parameters('trigger_every_30_sec_properties_pipeline_parameters_LastModified_From')]"
					},
					"trigger_every_30_sec_properties_pipeline_parameters_LastModified_To": {
						"value": "[parameters('trigger_every_30_sec_properties_pipeline_parameters_LastModified_To')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}